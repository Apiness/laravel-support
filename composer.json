{
	"name": "apiness/laravel-support",
	"description": "Support package for Laravel projects",
	"license": "MIT",
	"authors": [
		{
			"name": "Apiness",
			"email": "info@apiness.ch"
		}
	],
	"require": {
		"php": ">=5.5.9",
		"illuminate/support": "~5.0",
		"swiftmailer/swiftmailer": "~5.1"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0"
	},
	"autoload": {
		"psr-4": {
			"Apiness\\Support\\": "src/"
		},
		"files" : [
			"src/helpers.php"
		]
	},
	"autoload-dev": {
		"psr-4" : {
			"Tests\\" : "tests/"
		}
	}
}
