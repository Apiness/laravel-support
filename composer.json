{
	"name": "zaltana/support",
	"description": "Support package for Laravel projects",
	"license": "MIT",
	"authors": [
        {
            "name": "Nicolas Marfurt",
            "email": "nmarfurt@gmail.com"
        },
        {
            "name": "Nicolas Brosy",
            "email": "nicolas.brosy@gmail.com"
        }
	],
	"require": {
		"php": ">=5.6.0",
		"illuminate/support": "~5.0",
		"swiftmailer/swiftmailer": "~5.1"
	},
	"require-dev": {
		"phpunit/phpunit": "~5.0"
	},
	"autoload": {
		"psr-4": {
			"Zaltana\\Support\\": "src/"
		},
		"files" : [
			"src/helpers.php"
		]
	},
	"autoload-dev": {
		"psr-4" : {
			"Tests\\" : "tests/"
		}
	}
}
